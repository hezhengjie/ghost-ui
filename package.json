{
  "name": "ghost-ui",
  "version": "0.0.1",
  "description": "A  mobile ui lib  based Shadow DOM",
  "main": "lib/index.js",
  "module": "src/index.js",
  "postCompile": true,
  "scripts": {
    "build": "node build/build.js",
    "dev": "node build/dev-server.js",
    "doc:dev": "npm run dev & vuepress dev docs",
    "demo:dev": "npm run dev & node build/example/dev-server.js",
    "doc:build": "vuepress build docs",
    "demo:build": "node build/example/build.js",
    "doc-demo:build": "npm run doc:build && npm run demo:build",
    "release": "bash ./build/release/publish.sh",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --quiet --ext .js,.vue src -f stylish --color --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "files": [
    "src",
    "lib",
    "types"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hezhengjie/ghost-ui.git"
  },
  "homepage": "https://hezhengjie.github.com/hezhengjie/ghost-ui",
  "bugs": {
    "url": "https://github.com/hezhengjie/ghost-ui/issues"
  },
